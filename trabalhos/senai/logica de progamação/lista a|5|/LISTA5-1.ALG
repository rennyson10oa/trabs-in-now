Algoritmo "Lista5-1"
Var
   i : inteiro
Inicio
   i <- 100
   repita
      se (i % 2 = 0) entao
         escreval(i)
      fimse
      i <- i + 1
   ate (i>200)
   
   // -----------------------------
   escreval
   i <- 100
   repita
      escreval(i)
      i <- i + 2
   ate (i>200)


Fimalgoritmo