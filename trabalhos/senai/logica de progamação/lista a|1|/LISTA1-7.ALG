Algoritmo "Lista1-7"
Var
   n1, n2, n3, m : real
Inicio
   escreval("Digite o primeiro número")
   leia(n1)
   escreval("Digite o segundo número")
   leia(n2)
   escreval("Digite o terceiro número")
   leia(n3)
   m <- (n1+n2+n3)/3
   escreval("A média aritmética dos valores é ", m)
Fimalgoritmo